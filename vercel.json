{
    "version": 2,
    "builds": [
      {
        "src": "**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/assets/(.*)",
        "dest": "/assets/$1"
      },
      {
        "src": "/(.*)\\.css",
        "dest": "/$1.css"
      },
      {
        "src": "/(.*)\\.js",
        "dest": "/$1.js"
      },
      {
        "src": "/(.*)",
        "dest": "/$1.html"
      }
    ]
  }